<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <div class= "text-center">
        <% if params[:plan] == '2' %>
          <h1>Pro Account</h1>
        <% else %>
          <h1>Basic Account</h1>
        <% end %>
      </div>
      <div class="well">
        <!--Check for plan type-->
        <% if params[:plan] == '2' %>
         <%= render "pro_form" %>
        <% else %>
          <%= render "basic_form" %>
        <% end %>
        
        <%= render "devise/shared/links" %>
      </div>
    </div>
  </div>
</div>